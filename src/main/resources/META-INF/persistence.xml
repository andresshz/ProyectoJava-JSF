<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
    <persistence-unit name="default">
        <class>biblioteca.java.javabiblioteca.entities.AutoresEntity</class>
        <class>biblioteca.java.javabiblioteca.entities.CargoUsuariosEntity</class>
        <class>biblioteca.java.javabiblioteca.entities.EditorialesEntity</class>
        <class>biblioteca.java.javabiblioteca.entities.EstadoPrestamoEntity</class>
        <class>biblioteca.java.javabiblioteca.entities.GenerosEntity</class>
        <class>biblioteca.java.javabiblioteca.entities.LibrosEntity</class>
        <class>biblioteca.java.javabiblioteca.entities.PrestamosEntity</class>
        <class>biblioteca.java.javabiblioteca.entities.UsuariosEntity</class>


        <properties>
            <property name="eclipselink.jdbc.url" value="jdbc:mysql://localhost:3306/biblioteca_dwf"/>
            <property name="eclipselink.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
            <property name="eclipselink.jdbc.user" value="root"/>
        </properties>
    </persistence-unit>
</persistence>
